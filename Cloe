;Building GUI
(let [frame (new JFrame "Title")
      panel (new JPanel)
      label (new JLabel)]
  (doto panel 
   (.add label)
   (.setSize 500 500)
   (paintComponent [g]
     (proxy-super paintComponent g)
     (. g drawImage canvas nil nil)))
                   
  (doto frame
   (.add panel)
   (.setSize 500 500)
   (.setVisible true)))
      
